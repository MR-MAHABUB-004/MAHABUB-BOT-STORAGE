<script>
    function loadImage(imagepath, element) {
        imagepath = imagepath.replaceAll("&", "^$*");

        $.ajax({
            type: "GET",
            url: "/include/loadimage.php",
            data: "image_url=" + imagepath,
            beforeSend: function() {
                $(element).children(".btn-download").hide();
            },
            success: function(msg) {
                $(element).children(".post-image").attr("src", msg);
                $(element).children(".btn-download").attr("href", msg);
                $(element).children(".btn-download").show();
            }
        });
    }
</script>
<div class="mt-4">
